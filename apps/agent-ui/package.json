{
  "name": "@migration-planner-ui/agent-ui",
  "private": true,
  "version": "1.0.0-alpha",
  "type": "module",
  "scripts": {
    "start": "yarn run -T vite --host 127.0.0.1 --port 3001",
    "build": "yarn run -T tsc -b && yarn run -T vite build",
    "bundle": "yarn build && yarn pack --out ../../out/%s-%v.tgz",
    "clean": "rm -rf node_modules dist .tmp",
    "preview": "yarn run -T vite preview --host 127.0.0.1 --port 4001",
    "check": "yarn run -T biome check .",
    "check:fix": "yarn run -T biome check --write .",
    "test": "echo 'No tests configured for this package'"
  },
  "dependencies": {
    "@emotion/css": "^11.13.0",
    "@migration-planner-ui/agent-client": "workspace:*",
    "@migration-planner-ui/api-client": "workspace:*",
    "@patternfly/react-charts": "^8.0.0",
    "@patternfly/react-core": "^6.4.0",
    "@patternfly/react-icons": "^6.4.0",
    "@patternfly/react-table": "^6.4.0",
    "react": "^18.3.1",
    "react-content-loader": "^6.2.0",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.0",
    "react-use": "^17.5.1",
    "victory": "^37.3.6",
    "victory-core": "^37.3.6"
  },
  "files": [
    "dist"
  ]
}
