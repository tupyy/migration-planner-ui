{
  "$schema": "https://esm.sh/@openapitools/openapi-generator-cli@2.26.0/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.18.0",
    "generators": {
      "api-client": {
        "inputSpec": "https://raw.githubusercontent.com/kubev2v/migration-planner/main/api/v1alpha1/openapi.yaml",
        "output": "packages/api-client",
        "generatorName": "typescript-fetch",
        "additionalProperties": {
          "npmName": "@migration-planner-ui/api-client",
          "npmVersion": "1.0.0-alpha",
          "ensureUniqueParams": true,
          "supportsES6": true,
          "withInterfaces": true,
          "importFileExtension": ".js"
        }
      },
      "agent-client": {
        "inputSpec": "packages/agent-client/api/openapi.yaml",
        "output": "packages/agent-client",
        "generatorName": "typescript-fetch",
        "additionalProperties": {
          "npmName": "@migration-planner-ui/agent-client",
          "npmVersion": "1.0.0-alpha",
          "ensureUniqueParams": true,
          "supportsES6": true,
          "withInterfaces": true,
          "importFileExtension": ".js"
        }
      }
    }
  }
}
